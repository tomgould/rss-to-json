{
  "name": "tomgould/rsstojson",
  "description": "Universal RSS/Atom/XML parser that outputs clean JSON-compatible arrays.",
  "type": "library",
  "license": "MIT",
  "keywords": ["rss", "atom", "xml", "mrss", "feed", "parser", "json"],
  "authors": [
    {
      "name": "Tom Gould",
      "email": "your-email@example.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "TomGould\\RSSToJson\\": "src/"
    }
  },
  "bin": ["bin/rsstojson"],
  "require": {
    "php": ">=7.4",
    "ext-libxml": "*",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "scripts": {
    "test": "phpunit --bootstrap vendor/autoload.php tests"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}

